{
  "name": "kc-mvp-server",
  "version": "2.3.2",
  "description": "Karma Community MVP Server (NestJS + Postgres + Redis)",
  "private": true,
  "license": "MIT",
  "_TODO_DEPENDENCIES": "// TODO: Audit all dependencies for security vulnerabilities and outdated versions",
  "_TODO_TESTING": "// TODO: Add comprehensive testing framework and test scripts",
  "_TODO_DEPLOYMENT": "// TODO: Add proper deployment scripts and CI/CD integration",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rm -f *.tsbuildinfo && tsc -p tsconfig.build.json && npm run copy:schema",
    "copy:schema": "mkdir -p dist/database && cp -f src/database/schema.sql dist/database/ && cp -f src/database/challenges-schema.sql dist/database/ 2>/dev/null || true",
    "start": "node dist/main.js",
    "start:local": "REDIS_URL=redis://localhost:6379 POSTGRES_HOST=localhost POSTGRES_PORT=5432 POSTGRES_USER=kc POSTGRES_PASSWORD=kc_password POSTGRES_DB=kc_db EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=430191522654-o70t2qnqc4bvpvmbpak7unog7pvp9c95.apps.googleusercontent.com node dist/main.js",
    "start:dev": "GOOGLE_CLIENT_ID=430191522654-o70t2qnqc4bvpvmbpak7unog7pvp9c95.apps.googleusercontent.com EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=430191522654-o70t2qnqc4bvpvmbpak7unog7pvp9c95.apps.googleusercontent.com DATABASE_URL=postgresql://kc:kc_password@localhost:5432/kc_db REDIS_URL=redis://localhost:6379 NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "init:db": "ts-node -r tsconfig-paths/register src/scripts/init-db.ts",
    "add:google-id": "ts-node -r tsconfig-paths/register src/scripts/add-google-id-column.ts",
    "sync:firebase-users": "ts-node -r tsconfig-paths/register src/scripts/sync-firebase-users.ts",
    "data:export": "ts-node -r tsconfig-paths/register src/scripts/export-data.ts",
    "data:anonymize": "ts-node -r tsconfig-paths/register src/scripts/anonymize-data.ts",
    "data:import": "ts-node -r tsconfig-paths/register src/scripts/import-data.ts",
    "reset:db": "ts-node -r tsconfig-paths/register scripts/reset-db.ts",
    "reset:db:full": "./scripts/reset-db-full.sh",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "_TODO_ADD_SCRIPTS": "// TODO: Add missing scripts: test, test:watch, test:coverage, migration:run, migration:revert, seed, security-audit"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/throttler": "^5.2.0",
    "argon2": "^0.31.2",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "firebase-admin": "^12.7.0",
    "google-auth-library": "^10.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.2",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.15.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}